}

'
 
 
DELETE API CURL
 
curl --location --request DELETE 'http://1msg.1point1.in:3001/api/chat/bot/agents/15/delete/' \

--header 'Content-Type: application/json' \

--data '{

  "created_by": "123",

  "dialer": "auto",

  "flow_type": "sales",

  "agent_name": "Updated Agent Name",

  "agent_type": "AI",

  "sts_model_id": "sts123",

  "sts_model_perms": ["read", "write"],

  "llm_model_id": "gpt-4",

  "llm_model_param": {"temperature": 0.7, "max_tokens": 500},

  "llm_model_document": "doc001",

  "tts_model_id": "tts001",

  "tts_model_perm": ["read"]

}

'
 
